    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "cloudinary": "^2.4.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^5.7.2",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "mongoose-url-slugs": "^1.0.2",
    "paytmchecksum": "^1.5.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.1",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
