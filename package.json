  "scripts": {
    "predeploy": "concurrently \"set NODE_NO_WARNINGS=1 && node server/server.js\" \"npm run build\"",
    "deploy": "concurrently \"set NODE_NO_WARNINGS=1 && node server/server.js\" \"gh-pages -d build\"",
    "start": "npm run start:internal --silent",
    "start:internal": "set NODE_OPTIONS=--no-deprecation && set DISABLE_ESLINT_PLUGIN=true && set SKIP_PREFLIGHT_CHECK=true && concurrently \"node server/server.js\" \"react-scripts start\"",
    "build": "concurrently \"set NODE_NO_WARNINGS=1 && node server/server.js\" \"set GENERATE_SOURCEMAP=false && react-scripts build\"",
    "test": "concurrently \"set NODE_NO_WARNINGS=1 && node server/server.js\" \"react-scripts test\"",
    "eject": "concurrently \"set NODE_NO_WARNINGS=1 && node server/server.js\" \"react-scripts eject\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}